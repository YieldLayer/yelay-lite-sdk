{
	"name": "@yelay-lite/sdk",
	"main": "build/index",
	"types": "build/index.d.ts",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"version": "2.0.4-beta.0",
	"description": "Yelay Lite SDK",
	"repository": {
		"type": "git",
		"url": "https://github.com/YieldLayer/yelay-lite-sdk.git"
	},
	"files": [
		"build"
	],
	"scripts": {
		"build": "rimraf ./build && tsc",
		"test": "jest --config jest.config.ts",
		"version:beta:new": "npm version preminor --preid=beta --no-git-tag-version",
		"version:beta:patch": "npm version prepatch --preid=beta --no-git-tag-version",
		"version:beta:increment": "npm version prerelease --preid=beta --no-git-tag-version",
		"version:stable:patch": "npm version patch --no-git-tag-version",
		"version:stable:minor": "npm version minor --no-git-tag-version",
		"version:stable:major": "npm version major --no-git-tag-version",
		"release:tag": "git tag -a v$(node -p \"require('./package.json').version\") -m \"Release v$(node -p \"require('./package.json').version\")\"",
		"release:push": "git push origin main && git push origin --tags"
	},
	"keywords": [
		"yelay",
		"wallet",
		"yield",
		"blockchain",
		"ethereum"
	],
	"license": "ISC",
	"peerDependencies": {
		"@gud/drift": "^1.0.2"
	},
	"devDependencies": {
		"@types/node": "22.13.4",
		"@typescript-eslint/eslint-plugin": "8.17.0",
		"@typescript-eslint/parser": "8.17.0",
		"dotenv": "16.4.7",
		"rimraf": "6.0.1",
		"ts-node": "10.9.2",
		"typescript": "5.6.2"
	},
	"jest": {
		"testTimeout": 200000
	},
	"packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
}
