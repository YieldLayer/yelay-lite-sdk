{
	"name": "@yelay-lite/sdk",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"version": "1.0.21",
	"description": "Yelay Lite SDK",
	"main": "build/index.js",
	"types": "build/index.d.ts",
	"repository": {
