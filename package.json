	},
	"devDependencies": {
		"@typechain/ethers-v5": "11.1.2",
		"@types/jest": "29.5.14",
		"@types/lodash": "4.17.16",
		"@types/node": "22.13.4",
		"@typescript-eslint/eslint-plugin": "8.17.0",
		"@typescript-eslint/parser": "8.17.0",
		"dotenv": "16.4.7",
		"jest": "29.7.0",
		"ts-jest": "29.2.5",
